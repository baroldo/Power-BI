CustomerPercentileRank = 
VAR Rank =
    RANKX(
        ALLSELECTED(MARGIN_DECILIES),
        MARGIN_DECILIES[MARGIN_$],
        ,
        DESC,
        DENSE
    )

VAR TotalCustomers =
    COUNTROWS(ALLSELECTED(MARGIN_DECILIES))

RETURN
    ROUND(
        DIVIDE(Rank, TotalCustomers) * 100,
        0
    )
